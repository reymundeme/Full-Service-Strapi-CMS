{
  "kind": "collectionType",
  "collectionName": "child_pages",
  "info": {
    "singularName": "child-page",
    "pluralName": "child-pages",
    "displayName": "ChildPage"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "seoTitle": {
      "type": "text",
      "maxLength": 60
    },
    "seoDescription": {
      "type": "text",
      "maxLength": 160
    },
    "canonicalUrl": {
      "type": "text"
    },
    "keywords": {
      "type": "json"
    },
    "ogImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "jsonLd": {
      "type": "json"
    },
    "page": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::page.page",
      "inversedBy": "child_pages"
    }
  }
}
